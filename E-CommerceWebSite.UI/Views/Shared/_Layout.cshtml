<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1.0" />
    <title>@ViewBag.Title</title>

    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.0/css/all.css"
          integrity="sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ" crossorigin="anonymous">

    <link href="~/Content/lib/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/css/navbar.css" rel="stylesheet" />
    <link href="~/Content/css/index.css" rel="stylesheet" />
    @RenderSection("customStyles", false)

</head>
<body>
    @Html.Partial("_Navbar")
    <div class="container-fluid">
        @Html.Partial("_NavbarMenu")
    </div>
    <div class="container" style="background-color: #f8f9fa !important;">
        @RenderBody()
        <hr />
        @*<footer>
                <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
            </footer>*@
    </div>
    <script src="~/Content/lib/jQuery/jquery-3.3.1.js"></script>
    <script src="~/Content/lib/bootstrap/js/bootstrap.js"></script>

    @RenderSection("customScripts", false)

</body>
</html>